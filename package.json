{
  "name": "@researchgate/node-package-blueprint",
  "version": "0.1.1",
  "description": "Blueprint for scaffolding new node packages",
  "main": "lib/js/index.js",
  "scripts": {
    "build": "run-s build:*",
    "build:dist": "run-s build:dist:*",
    "build:dist:css": "echo build:dist:css",
    "build:lib": "run-s build:lib:*",
    "build:lib:js": "echo build:lib:js",
    "build:lib:es": "echo build:lib:es",
    "build:lib:scss": "echo build:lib:scss",
    "clean": "run-s clean:*",
    "clean:build": "echo clean:build",
    "clean:dist": "echo clean:dist",
    "clean:bin": "echo clean:bin",
    "coverage": "echo coverage",
    "format": "eslint --fix",
    "generate": "echo generate",
    "lint": "run-s lint:*",
    "lint:scss": "echo lint:scss",
    "lint:json": "echo lint:json",
    "lint:js": "eslint {src,test}/.",
    "precommit": "run-s lint test",
    "prepare": "run-s clean generate build",
    "prepublishOnly": "run-s test",
    "test": "jest",
    "watch": "echo watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/researchgate/node-package-blueprint.git"
  },
  "keywords": [
    "node",
    "npm",
    "scaffold"
  ],
  "author": "Roland Loesslein <roland.loesslein@researchgate.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/researchgate/node-package-blueprint/issues"
  },
  "homepage": "https://github.com/researchgate/node-package-blueprint#readme",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.5.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.6.1"
  },
  "dependencies": {}
}
